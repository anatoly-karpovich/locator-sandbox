{
  "type": "module",
  "scripts": {
    "build": "rimraf dist && tsc && tsc-alias",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --fix",
    "format": "prettier . --check",
    "format:write": "prettier . --write",
    "prepare": "husky",
    "prestart": "npm run build",
    "start": "node dist/app.js",
    "predev": "npm run build",
    "dev": "concurrently \"tsc -w\" \"tsc-alias -w\" \"nodemon dist/app.js\""
  },
  "dependencies": {
    "@babel/parser": "^7.28.5",
    "concurrently": "^9.2.1",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-winston": "^4.2.0",
    "inversify": "^7.10.8",
    "linkedom": "^0.18.12",
    "nodemon": "^3.1.11",
    "playwright": "^1.57.0",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.1.2",
    "uuid": "^9.0.1",
    "winston": "^3.18.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@babel/types": "^7.28.5",
    "@types/express": "^5.0.6",
    "@types/rimraf": "^3.0.2",
    "@eslint/js": "^9.18.0",
    "eslint": "^9.18.0",
    "globals": "^15.14.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.3.0",
    "prettier": "^3.4.2",
    "@types/uuid": "^10.0.0",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "typescript-eslint": "^8.20.0",
    "typescript": "^5.9.3"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx,json,md,yml,yaml}": [
      "prettier --write"
    ],
    "src/**/*.{ts,tsx,js,jsx}": [
      "eslint --fix"
    ]
  }
}
